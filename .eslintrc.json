{
  "plugins": [
    "react-hooks"
  ],
     "extends": [
       "react-app",
       "plugin:react/recommended"
     ],
     "rules": {
       "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
       "react-hooks/exhaustive-deps": "warn",
       "react/prop-types": [0],
       "no-unused-vars": [1],
       "no-console": [1]
     }
}